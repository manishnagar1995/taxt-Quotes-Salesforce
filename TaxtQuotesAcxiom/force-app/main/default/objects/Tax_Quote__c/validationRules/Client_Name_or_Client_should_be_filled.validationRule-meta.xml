<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Client_Name_or_Client_should_be_filled</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 (ISBLANK( Client_Name__c ) ), 
 (ISBLANK(  Clients__c ) )
)</errorConditionFormula>
    <errorMessage>Client or Client Name should be filled</errorMessage>
</ValidationRule>
