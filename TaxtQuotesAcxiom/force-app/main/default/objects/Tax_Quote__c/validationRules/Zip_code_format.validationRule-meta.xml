<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Zip_code_format</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(REGEX(Zip_Code__c , &quot;\\d{5}(-\\d{4})?&quot;))</errorConditionFormula>
    <errorMessage>Zip code must be in the &quot;99999&quot; or &quot;99999-9999&quot; format.</errorMessage>
</ValidationRule>
